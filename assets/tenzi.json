[
    {
        "song_number": 1,
        "title": "Mwokozi Umeokoa",
        "subtitle": "Precious Saviour",

        "stancer_1": [
            "Mwokozi umeokoa,",
            "Nimekuwa wako wewe.",
            "Damu imenisafisha;",
            "Sifa kwa mwana Kondoo."  
        ],

        "chorus": [
            "Utukufu, Halleluya!",
            "Sifa kwa Mwana Kondoo!",
            "Damu imenisafisha,",
            "Utukufu kwa Yesu!"
        ],

        "stancer_2": [
            "Nilijitahidi sana",
            "Ila sikupata raha;",
            "Bali kwa kumtegemea",
            "Nilipata Baraka."
        ],

        "stancer_3": [
            "Daima namwegemea",
            "Damu ikifanya kazi,",
            "Nikioga kwa chemichemi",
            "Itokayo Mwokozi."
        ],

        "stancer_4": [
            "Sasa nimewekwa wakfu;",
            "Nitaishi kwako wewe:",
            "Fahari nashuhudia",
            "Ya wokovu wa bure."
        ],

        "stancer_5": [
            "Nasimama kwake Yesu,",
            "Ameponya roho yangu;",
            "Ameniondoa dhambi,",
            "Anifanye mzima."
        ],

        "stancer_6": [
            "Nilikuwa kifungoni,",
            "Niliteswa na dhambi,",
            "Nilifungwa minyororo",
            "Yesu akanifungua."
        ],

        "stancer_7": [
            "Sifa, ameninunua!",
            "Sifa, nguvu za wokovu!",
            "Sifa, Bwana huhifadhi!",
            "Sifa zake milele."
        ]
    },

    {
        "song_number": 2,
        "title": "Twamsifu Mungu",
        "subtitle": "We praise Thee, Oh God",

        "stancer_1": [
            "Twamsifu Mungu",
            "kwa Mwana wa Pendo,",
            "Aliyetufia na kupaa juu."
        ],

        "chorus": [
            "Aleluya! Usifiwe; Aleluya! Amin.",
            "Aleluya! Usifiwe, utubariki."
        ],

        "stancer_2": [
            "Twamsifu Mungu",
            "kwa Roho Mtukufu,",
            "Ametufunulia Mwokozi wetu."
        ],

        "stancer_3": [
            "Twamsifu Mwana,",
            "aliyetufia,",
            "Aliyetwaa dhambi akazifuta."
        ],

        "stancer_4": [
            "Twamsifu Mungu",
            "wa neema yote,",
            "Ametukomboa akatuongoza."
        ],

        "stancer_5": [
            "Tuamshe tena,",
            "tujazena pendo,",
            "Na moyoni uwashe moto wa Roho."
        ]
    }
]